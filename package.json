{
    "name": "json2model",
    "displayName": "JSON to Model",
    "description": "Convert JSON to Model.",
    "version": "0.0.7",
    "publisher": "kyledh",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "json",
        "typescript",
        "c#",
        "model"
    ],
    "activationEvents": [
        "onCommand:j2m.toModel"
    ],
    "main": "./dist/extension",
    "contributes": {
        "commands": [
            {
                "command": "j2m.toModel",
                "title": "Convert JSON to Model"
            },
            {
                "command": "j2m.toTypeScript",
                "title": "Convert JSON to TypeScript Model"
            },
            {
                "command": "j2m.toCSharp",
                "title": "Convert JSON to C# Model"
            }
        ],
        "keybindings": [
            {
                "command": "j2m.toModel",
                "key": "cmd+alt+V"
            }
        ],
        "configuration": {
            "title": "JSON to Model",
            "properties": {
                "general.defaultConvertType": {
                    "type": "string",
                    "enum": [
                        "TypeScript",
                        "C#"
                    ],
                    "default": "TypeScript",
                    "description": "Default convert type."
                },
                "typescript.duplicateClass": {
                    "type": "boolean",
                    "default": false,
                    "description": "Generate duplicate class."
                },
                "typescript.generateSafeModel": {
                    "type": "boolean",
                    "default": true,
                    "description": "Generate safe model."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "webpack --mode none",
        "watch": "webpack --mode none --watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.12.21",
        "raw-loader": "^3.0.0",
        "ts-loader": "^6.0.4",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "webpack": "^4.35.2",
        "webpack-cli": "^3.3.5"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "deepmerge": "^3.3.0",
        "change-case": "^3.1.0",
        "pluralize": "^8.0.0",
        "@types/pluralize": "^0.0.29",
        "nunjucks": "^3.2.0",
        "@types/nunjucks": "^3.1.1"
    }
}
